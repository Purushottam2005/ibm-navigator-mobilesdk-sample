platform :ios, '8.4'

def corekit_pods
    pod 'CocoaLumberjack', '1.9.2'
    pod 'Alamofire',  '~> 4.0'
    pod 'Mantle', '2.1'
    pod 'MTLManagedObjectAdapter', '1.0.0.1'
end

xcodeproj 'IBMECMCoreSample/IBMECMCoreSample.xcodeproj'
target :'IBMECMCoreSampleApp' do
    corekit_pods
    use_frameworks!
    xcodeproj 'IBMECMCoreSample/IBMECMCoreSample.xcodeproj'
end

